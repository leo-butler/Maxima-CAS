
LOAD('DELTA)$
G:(X-1)*(X-2)*(X-3)*(X-4)*(X^2+1);
DELINT(F(X)*DELTA(G),X);
FACTOR(102);
DELINT(F(X)*DELTA(G),X,0,SQRT(3));
DELINT(F(Y)*DELTA(Y-X),Y,0,INF);
DELINT(F(Y)*DELINT(H(X)*DELTA(Y-2*X)*DELTA(X-3),X,0,INF),Y,0,INF);
DELINT(F(Y)*DELINT(H(X)*DELTA(X-2*Y)*DELTA(X),X),Y);
DELINT(F(Y)*DELTA(Y-A)+DELTA(Y-B)*O(2*Y),Y);
DELINT(COS(K*X)-DELTA(X)*J(C*X-B),X,-%PI,%PI);
