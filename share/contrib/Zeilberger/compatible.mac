/* Maxima-compatibility definitions */

/* RISC Institute, Linz, Austria    */
/* by Fabrizio Caruso               */


op(expr) := 
  if atom(expr) then
     expr
  else 
     part(expr,0);


operatorp(expr,operator) := 
    if op(expr)=operator then
       true
    else
       false;

solve_inconsistent_error : false;







