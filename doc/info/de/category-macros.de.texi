@macro opencatbox {}
@ifhtml
@html
<div class=categorybox>
@end html
@end ifhtml
@end macro

@macro category {name}
@ifhtml
@html
&middot;
@end html
@ref{Kategorie: \name\}
@end ifhtml
@end macro

@macro closecatbox {}
@ifhtml
@html
</div>
@end html
@end ifhtml
@end macro

@macro mref {nodename}
@ifhtml
@code{@ref{\nodename\}}
@end ifhtml
@ifnothtml
@code{\nodename\}
@end ifnothtml
@end macro

@macro mxref {nodename, text}
@ifhtml
@code{@ref{\nodename\, \text\}}
@end ifhtml
@ifnothtml
@code{\text\}
@end ifnothtml
@end macro


@macro mrefdot {nodename}
@ifhtml
@code{@ref{\nodename\}}.
@end ifhtml
@ifnothtml
@code{\nodename\}.
@end ifnothtml
@end macro

@macro mrefcomma {nodename}
@ifhtml
@code{@ref{\nodename\}},
@end ifhtml
@ifnothtml
@code{\nodename\},
@end ifnothtml
@end macro

@macro nref {nodename}
@ifhtml
@ref{\nodename\}
@end ifhtml
@ifnothtml
\nodename\
@end ifnothtml
@end macro

@macro nrefdot {nodename}
@ifhtml
@ref{\nodename\}.
@end ifhtml
@ifnothtml
\nodename\.
@end ifnothtml
@end macro

@macro nrefcomma {nodename}
