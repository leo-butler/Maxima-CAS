      [                                                      ]
      [ .031179   - .052842  - .10054   - .034033   .14834   ]
(%i7) z[2];  /* vector de correlacion multiple */
(%o7)       [.85063, .80634, .86474, .71867, .72675]
(%i8) z[3];  /* matriz de correlaciones parciales */
