  affiliation =  "Dept. of Math., Nijmegen Univ., Netherlands",
  classification = "C7310 (Mathematics computing); C6130 (Data handling
                 techniques)",
  keywords =     "Integral basis; Algebraic function field; Computer
                 algebra systems; Maple 5.2; AXIOM; Algebraic number
                 field; Puiseux expansions; Intermediate expression
                 swell",
  language =     "English",
  pubcountry =   "UK",
  thesaurus =    "Mathematics computing; Symbol manipulation",
}

